\RequirePackage{listings}
\RequirePackage{courier}

\mode<presentation>

% - Main color GUBLUE (Title, bullets, arrows)
% - First accent BLUE (math, or RED)
% - Second accent TEAL
% - Third accent GREEN
% - Fourth accent RED
% - Fifth accent YELLOW

\definecolor{mspp@gublue}{rgb}{0.016,0.118,0.259}
\definecolor{mspp@blue}{rgb}{0,0.239,0.647}
\definecolor{mspp@teal}{rgb}{0,0.71,0.886}
\definecolor{mspp@yellow}{rgb}{0.973,0.878,0.557}
\definecolor{mspp@green}{rgb}{0.392,0.655,0.043}
\definecolor{mspp@red}{rgb}{0.835,0,0.196}
\definecolor{mspp@gugray}{rgb}{0.388,0.4,0.416}
\definecolor{mspp@gray}{rgb}{0.733,0.737,0.737}
\definecolor{mspp@khaki}{rgb}{0.839,0.824,0.769}

\lstset{ %
  basicstyle=\ttfamily\footnotesize,
  backgroundcolor=\color{white},
  breakatwhitespace=true,
  breaklines=true,
  frame=single,
  rulecolor=\color{mspp@gray},
  commentstyle=\color{mspp@red},
  keywordstyle=\color{mspp@teal},
  stringstyle=\color{mspp@green},
  keepspaces=true,
  showspaces=false,
  showstringspaces=false,
  showtabs=false,
  tabsize=4,
  language=Python
}

\newcommand<>{\alertb}[1]{\setbeamercolor{alerted text}{fg=mspp@red}%
  \begin{alertenv}#2\relax#1\end{alertenv}%
  \setbeamercolor{alerted text}{fg=mspp@blue}%
}
\newcommand<>{\alertc}[1]{\setbeamercolor{alerted text}{fg=mspp@green}%
  \begin{alertenv}#2\relax#1\end{alertenv}%
  \setbeamercolor{alerted text}{fg=mspp@blue}%
}
\newcommand<>{\alertd}[1]{\setbeamercolor{alerted text}{fg=mspp@yellow}%
  \begin{alertenv}#2\relax#1\end{alertenv}%
  \setbeamercolor{alerted text}{fg=mspp@blue}%
}
\newcommand<>{\alerte}[1]{\setbeamercolor{alerted text}{fg=mspp@teal}%
  \begin{alertenv}#2\relax#1\end{alertenv}%
  \setbeamercolor{alerted text}{fg=mspp@blue}%
}


\setbeamercolor{normal text}{fg=black,bg=white}
\setbeamercolor{alerted text}{fg=mspp@blue}
